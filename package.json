{
  "name": "property-helpers",
  "version": "1.0.0",
  "description": "Simple helper functions for working with properties",
  "author": "Hezekiah Hendry <real.shadow.nova@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist node_modules/.cache",
    "lint": "eslint src tests --ext ts --fix",
    "format": "prettier --write {src,tests}/**/*.ts",
    "test": "jest",
    "build": "rollup -c rollup.bundle.ts",
    "update": "yarn upgrade-interactive",
    "prepublishOnly": "rollup-type-bundler",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@favware/npm-deprecate": "^1.0.4",
    "@favware/rollup-type-bundler": "^1.0.7",
    "@sapphire/eslint-config": "^4.3.3",
    "@sapphire/ts-config": "^3.3.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.70.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RealShadowNova/property-helpers.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/RealShadowNova/property-helpers/issues"
  },
  "homepage": "https://github.com/RealShadowNova/property-helpers",
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix --ext ts"
  }
}
