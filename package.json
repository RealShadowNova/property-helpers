{
  "name": "property-helpers",
  "version": "1.1.0",
  "description": "Simple helper functions for working with properties",
  "author": "Hezekiah Hendry <real.shadow.nova@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "clean": "rimraf dist node_modules/.cache",
    "lint": "eslint src tests --ext ts --fix",
    "format": "prettier --write {src,tests}/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsup && tsc -b src",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run",
    "postinstall": "husky install",
    "prepack": "yarn build && pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@favware/cliff-jumper": "^1.8.0",
    "@favware/npm-deprecate": "^1.0.4",
    "@sapphire/eslint-config": "^4.3.5",
    "@sapphire/ts-config": "^3.3.4",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.23",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-extended": "^2.0.0",
    "lint-staged": "^12.4.2",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^28.0.3",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RealShadowNova/property-helpers.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/RealShadowNova/property-helpers/issues"
  },
  "homepage": "https://github.com/RealShadowNova/property-helpers",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": "eslint --fix --ext ts"
  }
}
